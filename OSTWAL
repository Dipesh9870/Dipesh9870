"""""""""""""""""""""""""""""""""""""""""""""""""""""""""" KPL 4000
   SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_OPERATIONALACCTGDOCITEM WHERE PostingDate  BETWEEN @date2 AND @date3
      AND  GLAccount = '0001081030'
       and DebitCreditCode = 'H'
      AND FiscalYear = @fiscalyear AND CompanyCode = '4000'
     INTO  @data(loanreceivedkpl400_4).


   SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_OPERATIONALACCTGDOCITEM WHERE PostingDate  BETWEEN @date2 AND @date3
