   SELECT a~CompanyCode,
          a~FiscalYear,
          a~PostingDate,
          a~AccountingDocument,
          a~accountingdocumenttype,
          a~FinancialAccountType,
          a~GLAccount

     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode IN @com AND a~FiscalYear = @fiscalyear
               AND a~PostingDate  BETWEEN @date2 AND @date3
               INTO TABLE @data(it).

    SORT it  ASCENDING BY CompanyCode .
   DELETE ADJACENT DUPLICATES FROM it COMPARING AccountingDocument PostingDate CompanyCode  .
*   DELETE ADJACENT DUPLICATES FROM it COMPARING d-takanumber d-cust_stock_num d-materialcode d-batch .
*    DELETE it WHERE d-takanumber is INITIAL .
    READ TABLE it INTO DATA(wa) INDEX 1.


 DATA partyculars TYPE string.
 DATA company TYPE string.
 DATA company2 TYPE string.
 DATA company3 TYPE string.
 DATA company4 TYPE string.
 DATA company5 TYPE string.
*loop AT it INTO DATA(iv).

     SELECT SINGLE * FROM ZRELETED_PART_COMPANY1 WHERE CompanyCode = @wa-CompanyCode into @data(wa2).
     SELECT sum( amountincompanycodecurrency ) FROM I_OperationalAcctgDocItem WHERE CompanyCode = '4000' AND PostingDate  BETWEEN @date2 AND @date3
                    AND AccountingDocumentItemType = 'K' AND ( AccountingDocumentType = 'RE' or AccountingDocumentType = 'KG' ) into @data(ostwal).




