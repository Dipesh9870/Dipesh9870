      AND GLAccount = '0002151000  '
      AND FinancialAccountType = 'K' AND  CompanyCode = '4000'
          AND Supplier = '1500000100'
          INTO @DATA(loanrsipl_4).

              SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_OperationalAcctgDocItem WHERE PostingDate <= @date3
      AND GLAccount = '0002151000  '
      AND FinancialAccountType = 'K' AND  CompanyCode = '4000'
          AND Supplier = '7900000449'
          INTO @DATA(loanrnripl_4).





