"""""""""""""""""""""""""""""""""""""""""""""""""""""" KPL 4000
 SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_GLACCOUNTLINEITEM WHERE PostingDate  BETWEEN @date2 AND @date3
      AND Supplier = '0000020001'  AND Ledger = '0L'
      AND FiscalYear = @fiscalyear AND CompanyCode = '4000'
     AND OffsettingAccount IN ( '0004121040' , '0004121000' )
               INTO  @data(salarymdostwal_4) .

 SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_GLACCOUNTLINEITEM WHERE PostingDate  BETWEEN @date2 AND @date3
      AND Supplier = '0000020003'  AND Ledger = '0L'
      AND FiscalYear = @fiscalyear AND CompanyCode = '4000'
     AND OffsettingAccount IN ( '0004121040' , '0004121000' )
      INTO  @data(salarypramod_4) .

