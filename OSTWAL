      AND GLAccount = '0002151000'
      AND FinancialAccountType = 'K' AND  CompanyCode = '4000'
          AND Supplier IN ('1500000001' ,'1500000002' , '1500000003' ,  '1500000004', '1500000005' , '1500000029' , '1500000030' , '1500000031' , '1500000032' )
    INTO  @data(loanr200_4).

