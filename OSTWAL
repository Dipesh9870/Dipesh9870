""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""""" KPL 4000
   SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_OperationalAcctgDocItem WHERE PostingDate <= @date3
      AND GLAccount = '0002151000'
      AND FinancialAccountType = 'K' AND  CompanyCode = '4000'
          AND Supplier IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
     INTO  @data(loanr400_4).

SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_OperationalAcctgDocItem WHERE PostingDate <= @date3
      AND GLAccount = '0002151000'
      AND FinancialAccountType = 'K' AND  CompanyCode = '4000'
          AND Supplier IN ('1500000006' , '1500000007' , '1500000008', '1500000009' , '1500000010' , '1500000012' , '1500000013' , '1500000014' , '1500000015' , '1500000017' , '1500000080' )
    INTO  @data(loanr300_4).

