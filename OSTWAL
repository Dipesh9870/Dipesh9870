*************************************************************************NEW LOGIC
SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_GLACCOUNTLINEITEM WHERE PostingDate  BETWEEN @date2 AND @date3
      AND GLAccount = '0004161030' AND Ledger = '0L'
      AND FiscalYear = @fiscalyear AND CompanyCode = '4000'
      AND OffsettingAccount IN ('1500000006' , '1500000007' , '1500000008', '1500000009' , '1500000010' , '1500000012' , '1500000013' , '1500000014' , '1500000015' , '1500000017' , '1500000080' )
      INTO @DATA(intrestpaidmpapl300_4).



