CLASS zfi_releted_party_print_class DEFINITION
  PUBLIC
  FINAL
  CREATE PUBLIC .

  PUBLIC SECTION.


   CLASS-METHODS :

      read_posts
        IMPORTING VALUE(companycode)     TYPE string
                  VALUE(fiscalyear)      TYPE string
                  VALUE(fromdate)      TYPE string
                  VALUE(todate)      TYPE string
        RETURNING VALUE(result12) TYPE string
        RAISING   cx_static_check .

  PROTECTED SECTION.
  PRIVATE SECTION.
ENDCLASS.



CLASS ZFI_RELETED_PARTY_PRINT_CLASS IMPLEMENTATION.


METHOD read_posts .

    DATA date2  TYPE string.
    DATA gv1    TYPE string .
    DATA gv2    TYPE string .
    DATA gv3    TYPE string .

     gv3 = fromdate+0(2)  .
    gv2 = fromdate+3(2)  .
    gv1 = fromdate+6(4)  .

    CONCATENATE gv1 gv2 gv3   INTO date2.

    DATA date3 TYPE string.
    DATA gv4 TYPE string .
    DATA gv5 TYPE string .
    DATA gv6 TYPE string .

    gv6 = todate+0(2)  .
    gv5 = todate+3(2)  .
    gv4 = todate+6(4)  .

    CONCATENATE gv4 gv5 gv6  INTO date3.
  DATA: com TYPE RANGE OF bukrs.
     SPLIT companycode AT  ',' INTO TABLE DATA(itab).
    LOOP AT itab INTO  DATA(wa1).
      APPEND VALUE  #( sign = 'I' option = 'EQ' low = |{  wa1 alpha = in }|   ) TO com.
    ENDLOOP.


DATA lv_xml TYPE string.
DATA xsml TYPE string.
DATA n type STRING .
