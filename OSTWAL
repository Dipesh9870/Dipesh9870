    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
     AND customer IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
