*SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_GLACCOUNTLINEITEM WHERE PostingDate  BETWEEN @date2 AND @date3
*      AND GLAccount = '0004231280' AND Ledger = '0L'
*      AND FiscalYear = @fiscalyear AND CompanyCode = '1000'
*     AND OffsettingAccount IN ('1500000000' , '1500000011' , '1500000022', '1500000027' , '1500000028' , '1500000060')
*      INTO @data(RENTpaidsgfl_1).
SELECT SINGLE
* A~AccountingDocument  ,
 A~SUPPLIER,
 A~CompanyCodeCurrency,
 B~OffsettingAccount,
 A~FiscalYear,
 SUM( A~amountincompanycodecurrency  ) AS RENT_AMT
 FROM I_OPERATIONALACCTGDOCITEM AS A
 LEFT OUTER JOIN I_GLACCOUNTLINEITEM AS B ON ( B~Supplier =  A~Supplier AND B~AccountingDocument = A~AccountingDocument AND B~CompanyCode = A~CompanyCode
    AND   B~FiscalYear = A~FiscalYear  )
WHERE A~CompanyCode = '4000'  AND a~PostingDate  BETWEEN @date2 AND @date3 AND
      A~Supplier IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090') AND
      A~GLAccount = '0004231280'  and
      A~DebitCreditCode = 'S' and
      A~FiscalYear = @fiscalyear AND
*      a~AccountingDocumentType IN ( 'KR' , 'KG' ,'WE' ) AND
*      a~FinancialAccountType = 'S'
       B~Ledger = '0L'

      GROUP BY
*      A~AccountingDocument,
      A~SUPPLIER,
      A~CompanyCodeCurrency,
       B~OffsettingAccount,
       A~FiscalYear
        INTO @Data(RENTpaidkpl_4).


"""""""""""""""""""""""""""""""" KPL 4000
SELECT SINGLE
* A~AccountingDocument  ,
 A~SUPPLIER,
 A~CompanyCodeCurrency,
 B~OffsettingAccount,
 A~FiscalYear,
 SUM( A~amountincompanycodecurrency  ) AS RENT_AMT
 FROM I_OPERATIONALACCTGDOCITEM AS A
 LEFT OUTER JOIN I_GLACCOUNTLINEITEM AS B ON ( B~Supplier =  A~Supplier AND B~AccountingDocument = A~AccountingDocument AND B~CompanyCode = A~CompanyCode
    AND   B~FiscalYear = A~FiscalYear  )
WHERE A~CompanyCode = '4000'  AND a~PostingDate  BETWEEN @date2 AND @date3 AND
      A~Supplier IN ('1500000006' , '1500000007' , '1500000008', '1500000009' , '1500000010' , '1500000012' , '1500000013' , '1500000014' , '1500000015' , '1500000017' , '1500000080' ) AND
      A~GLAccount = '0004231280'  and
      A~DebitCreditCode = 'S' and
      A~FiscalYear = @fiscalyear AND
*      a~AccountingDocumentType IN ( 'KR' , 'KG' ,'WE' ) AND
*      a~FinancialAccountType = 'S'
       B~Ledger = '0L'

      GROUP BY
*      A~AccountingDocument,
      A~SUPPLIER,
      A~CompanyCodeCurrency,
       B~OffsettingAccount,
       A~FiscalYear
      INTO @Data(RENTpaidmbapl_4).

