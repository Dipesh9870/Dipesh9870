   SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND  a~FiscalYear = @fiscalyear
      AND a~PostingDate  BETWEEN @date2 AND @date3
     AND A~Customer IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
