**************************************** Trade Receivables ***********************************

"""""""""""""""""""""""""""""""""""""""""""""""""""""""" KPL 4000

SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND a~PostingDate <= @date3
    AND FinancialAccountType = 'D'
     AND A~Customer IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
        AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H'
*     AND ( A~SpecialGLCode Not in ( 'F', 'H' ) )
        INTO  @data(Tradere400_4).

SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
    AND customer IN ('1500000006' , '1500000007' , '1500000008', '1500000009' , '1500000010' , '1500000012' , '1500000013' , '1500000014' , '1500000015' , '1500000017' , '1500000080' )
                   AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H' INTO  @data(Tradere300_4) .
*
SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
     AND customer IN ('1500000001' ,'1500000002' , '1500000003' ,  '1500000004', '1500000005' , '1500000029' , '1500000030' , '1500000031' , '1500000032' )
    AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H'  INTO  @data(Tradere200_4) .
*
SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
     AND customer IN ('1500000000' , '1500000011' , '1500000022', '1500000027' , '1500000028' , '1500000060')
 AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H' INTO  @data(Tradere100_4) .
*
SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '4000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
     AND customer = '1500000100'
        AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H' INTO  @data(Traderesipl_4) .
*
*"""""""""""""""""""""""""""""""""""""""""""""""""""""""" MBPL 3000
SELECT SINGLE SUM( amountincompanycodecurrency )
    FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '3000' AND a~PostingDate <= @date3
     AND FinancialAccountType = 'D'
   AND customer IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
          AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE 'H'   INTO  @data(Tradere400_3) .
