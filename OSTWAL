SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000'  AND a~PostingDate <= @date3
               AND A~Supplier IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')               AND A~FinancialAccountType = 'K'
             AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE '7'  INTO  @data(Advance400_1) .
               IF Advance400_1 < 0.
                  Advance400_1 = ''.
                  ELSE.
                   Advance400_1 = Advance400_1.
                   ENDIF.

SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000'  AND a~PostingDate <= @date3
               AND A~Supplier IN ('1500000006' , '1500000007' , '1500000008', '1500000009' , '1500000010' , '1500000012' , '1500000013' , '1500000014' , '1500000015' , '1500000017' , '1500000080' )               AND A~FinancialAccountType = 'K'
             AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE '7'  INTO  @data(Advance300_1) .
               IF Advance300_1 < 0.
                  Advance300_1 = ''.
                  ELSE.
                   Advance300_1 = Advance300_1.
                   ENDIF.

SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000'  AND a~PostingDate <= @date3
               AND A~Supplier IN ('1500000001' ,'1500000002' , '1500000003' ,  '1500000004', '1500000005' , '1500000029' , '1500000030' , '1500000031' , '1500000032' )               AND A~FinancialAccountType = 'K'
              AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE '7' INTO  @data(Advance200_1) .
               IF Advance200_1 < 0.
                  Advance200_1 = ''.
                  ELSE.
                   Advance200_1 = Advance200_1.
                   ENDIF.

SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000'  AND a~PostingDate <= @date3
               AND A~Supplier IN ('1500000000' , '1500000011' , '1500000022', '1500000027' , '1500000028' , '1500000060')
               AND A~FinancialAccountType = 'K'
             AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE '7'  INTO  @data(Advance100_1) .
               IF Advance100_1 < 0.
                  Advance100_1 = ''.
                  ELSE.
                   Advance100_1 = Advance100_1.
                   ENDIF.

   SELECT SINGLE SUM( amountincompanycodecurrency )
     FROM I_OperationalAcctgDocItem AS a WHERE a~CompanyCode = '1000'  AND a~PostingDate <= @date3
               AND A~Supplier = '1500000100'
               AND A~FinancialAccountType = 'K'
              AND A~SpecialGLCode NE 'F' AND A~SpecialGLCode NE '7' INTO  @data(Advancesipl_1) .
               IF Advancesipl_1 < 0.
                  Advancesipl_1 = ''.
                  ELSE.
                   Advancesipl_1 = Advancesipl_1.
                   ENDIF.

*********************************** Advance to Supplier *******************************

*********************************  Loan Payable ***************************************

"""""""""""""""""""""""""""""""""""""""""""" KPL 4000
SELECT SINGLE
* A~AccountingDocument  ,
 A~SUPPLIER,
 SUM( amountincompanycodecurrency  ) AS RENT_AMT
 FROM I_OPERATIONALACCTGDOCITEM AS A
WHERE
A~CompanyCode = '4000' AND
GLAccount = '0001081030' AND
*A~Supplier IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090') AND
*A~DebitCreditCode = 'H' and
a~PostingDate <= @date3
*A~FiscalYear = @fiscalyear
*a~AccountingDocumentType IN ( 'KR' , 'KG' ) and
*A~FinancialAccountType = 'K'
GROUP BY
*A~AccountingDocument,
A~Supplier
INTO @Data(loanp400_4).

