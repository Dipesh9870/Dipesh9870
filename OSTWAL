""""""""""""""""""""""""""""""""""""""""""""""""" KPL 4000
SELECT SINGLE
 A~SUPPLIER,
 SUM( amountincompanycodecurrency  ) AS RENT_AMT
 FROM I_GLAccountLineItem AS A
WHERE PostingDate  BETWEEN @date2 AND @date3 AND
a~Ledger = '0L' and
A~OffsettingAccount NE '0003011040'  AND
A~CompanyCode = '4000' AND
A~DebitCreditCode = 'H' and
A~FiscalYear = @fiscalyear AND
A~SpecialGLCode = '7' AND
A~Supplier IN ( '1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090' )
GROUP BY
*A~AccountingDocument,
A~Supplier
INTO @Data(RepAgaiLoGivkpl400_4).

