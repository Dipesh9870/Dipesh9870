***************************************Royalty paid********************************************

 SELECT SINGLE SUM( amountincompanycodecurrency ) AS AMT  FROM I_GLACCOUNTLINEITEM WHERE PostingDate  BETWEEN @date2 AND @date3
      AND GLAccount = '0004231400' AND Ledger = '0L'
      AND FiscalYear = @fiscalyear AND CompanyCode = '4000'
     AND OffsettingAccount IN ('1500000018' , '1500000019' , '1500000020', '1500000021' , '1500000023' , '1500000024' , '1500000025' , '1500000026' , '1500000041' , '1500000090')
INTO @Data(Royaltykpl400_4).

