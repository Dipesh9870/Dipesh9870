 LEFT OUTER JOIN I_GLACCOUNTLINEITEM AS B ON ( B~Supplier =  A~Supplier AND B~AccountingDocument = A~AccountingDocument AND B~CompanyCode = A~CompanyCode
    AND   B~FiscalYear = A~FiscalYear  )
WHERE A~CompanyCode = '3000'  AND a~PostingDate  BETWEEN @date2 AND @date3 AND
      A~Supplier IN ('1500000000' , '1500000011' , '1500000022', '1500000027' , '1500000028' , '1500000060') AND
      A~GLAccount = '0004231280'  and
      A~DebitCreditCode = 'S' and
      A~FiscalYear = @fiscalyear AND
*      a~AccountingDocumentType IN ( 'KR' , 'KG' ,'WE' ) AND
*      a~FinancialAccountType = 'S'
       B~Ledger = '0L'

      GROUP BY
*      A~AccountingDocument,
      A~SUPPLIER,
      A~CompanyCodeCurrency,
       B~OffsettingAccount,
       A~FiscalYear
      INTO @data(RENTpaidsgfl_3).


